<p id="notice"><%= notice %></p>

<h1>Availavilities</h1>

<table>
  <thead>
    <tr>
      <th>Day of week</th>
      <th>Begin hour</th>
      <th>End hour</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @availavilities.each do |availavility| %>
      <tr>
        <td><%= availavility.day_of_week %></td>
        <td><%= availavility.begin_hour %></td>
        <td><%= availavility.end_hour %></td>
        <td><%= availavility.team %></td>
        <td><%= link_to 'Show', availavility %></td>
        <td><%= link_to 'Edit', edit_availavility_path(availavility) %></td>
        <td><%= link_to 'Destroy', availavility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Availavility', new_availavility_path %>
